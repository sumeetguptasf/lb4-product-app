NODE_ENV=local
LOG_LEVEL=info
PORT=3004
DATABASE_URL=postgres://postgres:postgres@localhost:5432/authdb
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=authdb
DB_SYNCHRONIZE=true
DB_LOGGING=true
DB_MIGRATIONS=true
DB_MIGRATION_RUN=true
DB_MIGRATION_DIR=./migrations
DB_MIGRATION_TABLE=auth_migrations
DB_MIGRATION_FILE_NAME=auth_migration.json
DB_MIGRATION_TIMESTAMP_FORMAT=YYYYMMDDHHmmss
DB_MIGRATION_FILE_EXTENSION=.ts
DB_MIGRATION_FILE_NAME_PREFIX=auth_migration
DB_MIGRATION_FILE_NAME_SUFFIX=.ts
DB_MIGRATION_FILE_NAME_PATTERN=auth_migration_YYYYMMDDHHmmss.ts

REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=redis
REDIS_HOST=localhost
REDIS_PORT=6379
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=1h
JWT_ISSUER=your_jwt_issuer
JWT_AUDIENCE=your_jwt_audience
REDIS_DATABASE=0

AUTH_MIGRATION_COPY=true
SOURCELOOP_MIGRATION_COPY=true